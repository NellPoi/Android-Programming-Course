<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nellpoi.androidprogrammingcourse">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidProgrammingCourse"
        tools:targetApi="31">
        <activity
            android:name=".SubActivity_StateListDrawable"
            android:exported="false"
            android:label="@string/sub_StateListDrawable" />
        <activity
            android:name=".SubActivity_Resources"
            android:exported="false"
            android:label="@string/sub_Resources" />
        <activity
            android:name=".SubActivity_Spinner"
            android:exported="false"
            android:label="@string/sub_Spinner" />
        <activity
            android:name=".SubActivity_ListView"
            android:exported="false"
            android:label="@string/sub_ListView" />
        <activity
            android:name=".SubActivity_Guideline"
            android:exported="false"
            android:label="@string/sub_Guideline" />
        <activity
            android:name=".SubActivity_ConstraintLayout"
            android:exported="false"
            android:label="@string/sub_ConstraintLayout" />
        <activity
            android:name=".SubActivity_RelativeLayout"
            android:exported="false"
            android:label="@string/sub_RelativeLayout" />
        <activity
            android:name=".SubActivity_TableLayout"
            android:exported="false"
            android:label="@string/sub_TableLayout" />
        <activity
            android:name=".SubActivity_LinearLayout"
            android:exported="false"
            android:label="@string/sub_LinearLayout" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>